{
  "apps": [
    {
      "name": "hong-kong-cycling-map",
      "script": "npm",
      "args": "start",
      "instances": 3,
      "exec_mode": "cluster",
      "max_memory_restart": "256M",
      "cron_restart": "10 16 * * *",
      "listen_timeout": 10000,
      "kill_timeout": 10000,
      "env": {
        "NODE_ENV": "production"
      }
    }
  ]
}
